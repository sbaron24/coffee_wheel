<div class='main-details'>
  <h3><%= @coffee.name %></h3>
  <h5>by <%= @coffee.roaster %> and <%= @coffee.producer %></h5>
  <h6>from <%= @coffee.country %>, <%= @coffee.region %></h6>
  <p>Process: <%= @coffee.process %>, Altitude: <%= @coffee.altitude %></p>
</div>

<% if !current_user.nil? %>
  <div id='app'></div>
<% else %>
  <%= link_to 'Sign in to access attribute profile', new_user_session_path %>
<% end %>
